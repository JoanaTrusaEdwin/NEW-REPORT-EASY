<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NEW_REPORT_EASY.ADMIN.ViewUserFeedback"
             Title="ViewUserFeedback">
    <StackLayout Padding="20">

        <!-- Notification Title -->
        <Label Text="User Feedbacks" FontFamily="Roboto-Bold" FontAttributes="Bold" FontSize="24" TextColor="Black" HorizontalOptions="CenterAndExpand" Margin="0,0,0,20"/>

        <!-- Notification Content -->
        <Label Text="User Name: " FontFamily="Roboto" FontSize="18" TextColor="Black" Margin="0,0,0,20"/>
        <Label Text="Email: " FontFamily="Roboto" FontSize="18" TextColor="Black" Margin="0,0,0,20"/>
        <Label Text="Feedback: " FontFamily="Roboto" FontSize="18" TextColor="Black" Margin="0,0,0,20"/>
        
        



    </StackLayout>
</ContentPage>